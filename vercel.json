{
  "version": 2,
  "builds": [
    {
      "src": "src/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/ui/dist/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
